#pragma once
#include<string>

using namespace std;

namespace commands {

class DrawingCommand {

};

class CommandStart: DrawingCommand {
	string name;
	unsigned width;
	unsigned height;

	CommandStart(const string &name_,const unsigned width_,const unsigned height_) {
		name = name_;
		width = width_;
		height = height_;

	}
};

class CommandCircle: DrawingCommand {
	unsigned radius;
	unsigned centerX;
	unsigned centerY;

};

class CommandExit: DrawingCommand {
};

}
